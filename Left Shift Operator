package com.company.BitManupulation;

public class BitwiseOperatorLeftShift {
    public static void main(String[] args) {
        int x = 10;
        System.out.println(x<<1);  // x * 2^1
        System.out.println(x<<2);  // x * 2^2

        /*
        for(int i=0; i<=50; i++){
            System.out.print(i + " ");
            System.out.println(x<<i);

        }
         */

    }
}
