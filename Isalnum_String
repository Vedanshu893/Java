public class Solution {
    public int solve(ArrayList<Character> A) {

        int count =0;

        for(int i=0; i<A.size(); i++){
            
            char c = A.get(i);

            if((c>='0' && c<='9') || (c>='a' && c<='z') || (c>='A' && c<='Z')){
                count += 1;
            }

        }

       if(count == A.size()){
           return 1;
       }
       else{
           return 0;
       }
    }
}
