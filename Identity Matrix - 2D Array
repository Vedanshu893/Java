public class Solution {
    // DO NOT MODIFY THE LIST. IT IS READ ONLY
    public int solve(final List<ArrayList<Integer>> A) {
        int row = A.size();
        for(int i =0; i<A.size();i++){
            for(int j=0; j<A.get(i).size();j++){
                if(i==j && A.get(i).get(j)==1){
                    return 1;
                }
            }
      
        }
              return 0;
      
    }
}
