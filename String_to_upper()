public class Solution {
    public ArrayList<Character> to_upper(ArrayList<Character> A) {

        

        for(int i=0; i<A.size(); i++){
            char c = A.get(i);

            if(c<='z' && c>='a'){
                c = (char)(c^32);
                A.set(i,c);
            }

        }

        return A;
    }
}
