public class Solution {
    public int solve(ArrayList<Integer> A, int B) {
        int count =0;
        for(int i=0; i<A.size()-1; i++){
            for(int j=i+1; j<A.size();j++){
                if(A.get(i)+ A.get(j)==B){
                    
                    return 1;
                }
                
            }
        }
        return 0;

    }
}
