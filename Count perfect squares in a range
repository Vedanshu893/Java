package com.company.Maths;

public class PerfectSquareCount {
    public static void main(String[] args) {
        int count =0;
        int N = 50;

        for(int i=1; i*i<=N; i++){
            if(i*i <= N){
                count +=1;
            }
        }
        System.out.println(count);
    }
}
