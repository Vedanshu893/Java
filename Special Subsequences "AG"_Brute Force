// T.C :: O[N^2]
public class Solution {
    public int solve(String A) {
        int count = 0;

        for(int i=0; i<A.length(); i++){
            if(A.charAt(i) == 'A'){
                for(int j=i+1; j<A.length(); j++){
                    if(A.charAt(j)== 'G'){
                        count += 1;
                        
                    }
                }
            }
        }

        return count;
    }
}
